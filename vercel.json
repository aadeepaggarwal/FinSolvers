{
  "version": 2,
  "builds": [
    {
      "src": "flask_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/hackrx/run",
      "dest": "flask_app.py"
    },
    {
      "src": "/health",
      "dest": "flask_app.py"
    },
    {
      "src": "/(.*)",
      "dest": "flask_app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}
